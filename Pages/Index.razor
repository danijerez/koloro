@page "/"
@using MudBlazor
@using Excubo.Blazor.Canvas
@using koloro.Utils
@inject IJSRuntime js

<MudAppBar Color="Color.Tertiary" Class="menu_top">

</MudAppBar>

<MudCard Class="grid">
    <Canvas id="helper_canvas" @ref="helper_canvas" width="475" height="500" hidden/>
    <img id="test" src=@url>
    <MudIconButton Icon="@Icons.Material.Filled.FileDownload" Color="Color.Secondary" OnClick="() => { DownloadImage(url); }"></MudIconButton>
</MudCard>

@code{
    private Canvas helper_canvas;
    private string url = "data:image/png;base64,";

    private async Task DownloadImage(string url)
    {

        await using (var ctx = await helper_canvas.GetContext2DAsync())
        {
            await ctx.ClearRectAsync(0, 0, 475, 500);
            await ctx.FontAsync("25px solid");
            await ctx.FillTextAsync("test", 0, 475);
            await ctx.FilterAsync("saturate(0%) grayscale(100%) brightness(200%) contrast(1000%)");
            await ctx.DrawImageAsync("test", 0, 0);
            StateHasChanged();
        }

        await ImageUtils.FilterImage(js, "test");
    }

}