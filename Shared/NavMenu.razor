@using MudBlazor
@inject NavigationManager NavManager

<MudAppBar Style="@($"color:{Colors.Shades.White};background:{Colors.Grey.Darken4};")" Fixed="true">
    <MudIconButton Color="Color.Inherit" Icon="@Icons.Material.Filled.ColorLens" Link=@(NavManager.BaseUri)></MudIconButton>
    <MudButton Variant="Variant.Text"  Color="Color.Inherit" StartIcon="@Icons.Material.Filled.CatchingPokemon" Link="@(NavManager.BaseUri+"pokemon")" >Pokemon</MudButton>
    <MudSpacer />
    <MudIconButton Variant="Variant.Text" Icon="@Icons.Custom.Brands.GitHub" Target="_blank" Color="Color.Inherit" Link="https://github.com/danijerez" />
</MudAppBar>
